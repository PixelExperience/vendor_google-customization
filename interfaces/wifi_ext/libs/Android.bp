cc_prebuilt_library_shared {
        name: "vendor.google.wifi_ext@1.0",
        owner: "google",
        strip: {
                none: true,
        },
        target: {
                android_arm64: {
                        srcs: ["vendor.google.wifi_ext@1.0.so"],
                },
        },
        compile_multilib: "64",
        prefer: true,
}

cc_prebuilt_library_shared {
        name: "libwifi-pex",
        owner: "google",
        strip: {
                none: true,
        },
        target: {
                android_arm64: {
                        srcs: ["libwifi-pex.so"],
                },
        },
        compile_multilib: "64",
        prefer: true,
}

cc_prebuilt_binary {
        name: "vendor.google.wifi_ext@1.0-service-vendor",
        owner: "google",
        srcs: ["vendor.google.wifi_ext@1.0-service-vendor"],
        init_rc: ["vendor.google.wifi_ext@1.0-service.rc"],
        relative_install_path: "hw",
        overrides: ["android.hardware.wifi@1.0-service"],
        vendor: true,
}

cc_prebuilt_binary {
        name: "google.wifi_ext@1.0-service",
        owner: "google",
        srcs: ["vendor.google.wifi_ext@1.0-service-vendor"],
        init_rc: ["google.wifi_ext@1.0-service.rc"],
        relative_install_path: "hw",
        overrides: ["android.hardware.wifi@1.0-service"],
}
